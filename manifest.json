{
    "name": "Enhanced Dashboard",
    "version": "1.0",
    "description": "Improved Classroom info",
    "manifest_version": 3,
    "options_page":"options.html",
    "permissions": [
        "nativeMessaging",
        "storage",
        "scripting"
    ],
    "host_permissions":[
        "https://classroom.google.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service-worker": ["background.js"]
    },
    "icons": {
        "128": "logo.png"
    }
}